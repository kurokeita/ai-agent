{
	"name": "@kurokeita/add-skill",
	"version": "0.0.1",
	"description": "CLI to install AI agent skills to various platforms",
	"type": "module",
	"bin": {
		"add-skill": "./dist/bin/skills.js"
	},
	"main": "dist/bin/skills.js",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsx bin/skills.ts",
		"build": "tsc && tsx scripts/copy-assets.ts",
		"prepublishOnly": "pnpm run build",
		"lint": "pnpm run lint:ts && pnpm run lint:md",
		"lint:ts": "biome check .",
		"lint:md": "markdownlint '**/*.md' --ignore node_modules",
		"format": "biome check --write .",
		"release": "semantic-release"
	},
	"keywords": [
		"ai",
		"skills",
		"agent",
		"cli"
	],
	"author": "Kuro Keita",
	"repository": {
		"type": "git",
		"url": "https://github.com/kurokeita/ai-agent.git"
	},
	"license": "MIT",
	"packageManager": "pnpm@10.28.0",
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"commander": "^14.0.2",
		"fs-extra": "^11.3.3",
		"picocolors": "^1.1.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@semantic-release/git": "^10.0.1",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^25.0.9",
		"markdownlint-cli": "^0.47.0",
		"semantic-release": "^25.0.2",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3"
	},
	"publishConfig": {
		"access": "public"
	},
	"pnpm": {
		"overrides": {
			"undici": "^6.23.0"
		}
	}
}
